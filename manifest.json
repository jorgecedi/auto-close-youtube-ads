{
  "manifest_version": 2,
  "name": "Youtube Ads Autoskip",
  "version": "0.0.1",
  "description": "Simple extension for skip ads automatically",
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "https://www.youtube.com/"
  ]
}
